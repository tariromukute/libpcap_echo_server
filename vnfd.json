{
    "lifecycle_event":[
        {
            "event":"INSTANTIATE",
            "lifecycle_events":[
                "ansible/events/instantiate/site.yml"
            ]
        },
        {
            "event":"CONFIGURE",
            "lifecycle_events":[
                "ansible/events/configure/site.yml"
            ]
        },
        {
            "event":"START",
            "lifecycle_events":[
                 "ansible/events/operate/start.yml"
            ]
        },
        {
            "event":"STOP",
            "lifecycle_events":[
                 "ansible/events/operate/stop.yml"
            ]
        },
        {
            "event":"TERMINATE",
            "lifecycle_events":[
                 "ansible/events/terminate/site.yml"
            ]
        }
    ]
  }